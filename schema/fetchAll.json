{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Request all objects matching the pattern",
  "type": "object",
  "additionalProperties": false,
  "minProperties": 1,
  "properties": {
    "request": {
      "type": "object",
      "properties": {
        "role": {
          "description": "Objects of which role to fetch?",
          "type": "string"
        }
      }
    },
    "response": {
      "type": "object",
      "required": [ "objects" ],
      "additionalProperties": false,
      "properties": {
        "objects": {
          "$ref": "https://raw.githack.com/aleventhal/js2at/master/schema/ref/node-list.json"
        }
      }
    }
  }
}
